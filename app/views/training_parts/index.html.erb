<div>
  <h1 class="font-bold text-4xl">TrainingParts#index</h1>
  <p>Find me in app/views/training_parts/index.html.erb</p>
  <h1>鍛えたい部位を選択してください</h1>

  <%= form_with url: search_videos_path, method: :get, local: true do %>
    <div>
      <%= label_tag :training_part_id, "部位" %>
      <%= select_tag :training_part_id, options_from_collection_for_select(@training_parts, :id, :name) %>
    </div>
    <div>
      <%= submit_tag "検索" %>
    </div>
  <% end %>
</div>
